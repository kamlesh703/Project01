package Whilecond;

public class Printevennumbersusingwhile {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		
		//WAP to print even numbers between 20 to 30 using while condition.
		
		
		int i= 20;
		
		while(i<=30)
			
		{
			if(i%%2==0)
				
			{
				System.out.println(i);
				i++;
			}
		}
	}

}
